{
	"extends": "eslint:recommended",
	"env": {
		"node": true,
		"es2021": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
        "sourceType": "module"
	},
	"rules": {
        "indent": ["error", "tab"],
        "no-console": "off",
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
	}
}